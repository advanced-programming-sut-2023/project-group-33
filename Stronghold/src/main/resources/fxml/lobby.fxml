<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="900.0" prefWidth="1500.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.GUIController.LobbyGUI">
   <children>
      <Label alignment="CENTER" layoutX="725.0" layoutY="66.0" prefHeight="17.0" prefWidth="186.0" text="Lobby">
         <font>
            <Font name="American Typewriter" size="36.0" />
         </font>
      </Label>
      <ScrollPane layoutX="430.0" layoutY="142.0" prefHeight="468.0" prefWidth="776.0">
         <content>
            <VBox fx:id="VBox" prefHeight="465.0" prefWidth="758.0" />
         </content>
      </ScrollPane>
      <Button layoutX="736.0" layoutY="730.0" mnemonicParsing="false" onAction="#createGame" text="Create Game">
         <font>
            <Font name="American Typewriter" size="24.0" />
         </font>
      </Button>
      <ChoiceBox fx:id="playerNumberChoice" layoutX="890.0" layoutY="672.0" prefHeight="22.0" prefWidth="67.0" />
      <Label alignment="CENTER" layoutX="614.0" layoutY="671.0" prefHeight="28.0" prefWidth="247.0" text="Number of players:">
         <font>
            <Font name="American Typewriter" size="24.0" />
         </font>
      </Label>
      <Button layoutX="735.0" layoutY="797.0" mnemonicParsing="false" onAction="#back" prefHeight="44.0" prefWidth="180.0" text="Back">
         <font>
            <Font name="American Typewriter" size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
